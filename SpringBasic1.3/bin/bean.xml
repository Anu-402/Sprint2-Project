<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns:context="http://www.springframework.org/schema/context"
	xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans.xsd
		http://www.springframework.org/schema/context http://www.springframework.org/schema/context/spring-context-4.3.xsd">
	<bean id="sbu" class="com.cg.bean.SBU">
	<property name="sbuCode" value="100"></property>
	<property name="sbuName" value="Product Engineering Services"></property>
	<property name="sbuHead" value="Aneela"></property>
	<property name="emplist" >
	<list>
	<bean class="com.cg.bean.Employee">
	<property name="employeeId" value="101"></property>
	<property name="employeeName" value="Ananya"></property>
	<property name="salary" value="90000"></property>
	</bean>
	<bean class="com.cg.bean.Employee">
	<property name="employeeId" value="102"></property>
	<property name="employeeName" value="Sai"></property>
	<property name="salary" value="50000"></property>
	</bean>
	</list>
	</property>
	</bean>

</beans>
